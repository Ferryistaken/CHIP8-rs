{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run -- --rom roms/test_opcode.ch8",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo run"
		},
		{
			"type": "cargo",
			"command": "run --release -- --rom roms/test_opcode.ch8",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo run (release)"
		},
		{
			"type": "cargo",
			"command": "test",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo test"
		}
	]
}